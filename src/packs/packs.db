rice":0.5},"flags":{"_sheetTab":"description"},"img":"systems/dnd5e/icons/items/inventory/meat.jpg","_id":8,"totalWeight":60,"isBackpack":false},{"type":"loot","name":"Tinderbox","permission":{"default":0},"data":{"description":{"value":"<p>This small container holds flint, fire steel, and tinder (usually dry cloth soaked in light oil) used to kindle a fire. Using it to light a torch - or anything else with abundant, exposed fuel - takes an action. Lighting any other fire takes 1 minute.</p>\n<p>&nbsp;</p>"},"source":null,"quantity":3,"weight":1,"price":"0.5","attuned":null,"equipped":null,"rarity":"Common","attributes":{"spelldc":10},"marketPrice":0.5},"flags":{},"img":"systems/dnd5e/icons/items/inventory/torch.jpg","_id":9,"totalWeight":3,"isBackpack":false},{"type":"loot","name":"Torch","permission":{"default":0},"data":{"description":{"value":"<p>A torch burns for 1 hour, providing bright light in a 20-foot radius and dim light for an additional 20 feet. If you make a melee attack with a burning torch and hit, it deals 1 fire damage.</p>"},"source":null,"quantity":30,"weight":1,"price":"0.01","attuned":null,"equipped":null,"rarity":"Common","attributes":{"spelldc":10},"marketPrice":0.01},"flags":{"adnd5e":{"itemInfo":{"type":"item","idx":7}},"_sheetTab":"description"},"img":"systems/dnd5e/icons/items/inventory/torch.jpg","_id":10,"totalWeight":30,"isBackpack":false},{"type":"loot","name":"Waterskin","permission":{"default":0},"data":{"description":{"value":"<p>Can hold up to 4 pints of liquid</p>"},"source":null,"quantity":3,"weight":1,"price":"0.2","attuned":null,"equipped":null,"rarity":"Common","attributes":{"spelldc":10},"marketPrice":0.2},"flags":{},"img":"systems/dnd5e/icons/items/inventory/waterskin.jpg","_id":11,"totalWeight":3,"isBackpack":false}],"importSpells":false,"version":0.4,"markup":0},"core":{"sheetClass":"dnd5e.ItemSheetShop"}},"img":"systems/dnd5e/icons/items/inventory/backpack.jpg","_id":"x6Mt48Cjbk8c0hvo"}
